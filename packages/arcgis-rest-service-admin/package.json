{
  "name": "@esri/arcgis-rest-service-admin",
  "version": "2.10.1",
  "description": "Service administration helpers for @esri/arcgis-rest-js",
  "main": "dist/node/index.js",
  "unpkg": "dist/umd/service-admin.umd.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@esri/arcgis-rest-types": "^2.10.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@esri/arcgis-rest-auth": "^2.10.1",
    "@esri/arcgis-rest-portal": "^2.10.1",
    "@esri/arcgis-rest-request": "^2.10.1"
  },
  "peerDependencies": {
    "@esri/arcgis-rest-auth": "^2.0.0",
    "@esri/arcgis-rest-portal": "^2.0.0",
    "@esri/arcgis-rest-request": "^2.0.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc --module es2015 --outDir ./dist/esm --declaration",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "build:node": "tsc --module commonjs --outDir ./dist/node",
    "dev:esm": "tsc -w --module es2015 --outDir ./dist/esm --declaration",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js",
    "dev:node": "tsc -w --module commonjs --outDir ./dist/node"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/arcgis-rest-js.git",
    "directory": "packages/arcgis-rest-service-admin"
  },
  "contributors": [
    {
      "name": "Mike Tschudi",
      "email": "mtschudi@esri.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/Esri/arcgis-rest-js/issues"
  },
  "homepage": "https://github.com/Esri/arcgis-rest-js#readme",
  "keywords": [
    "typescript",
    "promise",
    "fetch",
    "arcgis",
    "esri",
    "ES6"
  ]
}
