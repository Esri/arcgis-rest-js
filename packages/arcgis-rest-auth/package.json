{
  "name": "@esri/arcgis-rest-auth",
  "version": "3.0.5",
  "description": "Authentication helpers for @esri/arcgis-rest-js.",
  "main": "dist/node/index.js",
  "unpkg": "dist/umd/auth.umd.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@esri/arcgis-rest-types": "^3.0.5",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@esri/arcgis-rest-request": "^3.0.5"
  },
  "peerDependencies": {
    "@esri/arcgis-rest-request": "^3.0.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "rimraf dist",
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc --module es2015 --outDir ./dist/esm --declaration",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "build:node": "tsc --module commonjs --outDir ./dist/node",
    "dev:esm": "tsc -w --module es2015 --outDir ./dist/esm --declaration",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js",
    "dev:node": "tsc -w --module commonjs --outDir ./dist/node"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/arcgis-rest-js.git",
    "directory": "packages/arcgis-rest-auth"
  },
  "contributors": [
    {
      "name": "Patrick Arlt",
      "email": "parlt@esri.com",
      "url": "http://patrickarlt.com/"
    }
  ],
  "bugs": {
    "url": "https://github.com/Esri/arcgis-rest-js/issues"
  },
  "homepage": "https://github.com/Esri/arcgis-rest-js#readme",
  "keywords": [
    "typescript",
    "promise",
    "fetch",
    "arcgis",
    "esri",
    "ES6"
  ],
  "esri": {
    "keyExports": [
      "UserSession",
      "ApplicationSession"
    ]
  }
}
