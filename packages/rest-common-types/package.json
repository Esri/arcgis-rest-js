{
  "name": "@esri/rest-common-types",
  "version": "0.0.0",
  "description": "Common types for @esri/rest-js.",
  "types": "dist/types/index.d.ts",
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:esm",
    "build:esm": "dir ./src/*.ts /b /s > ts-files.txt && tsc @ts-files.txt --moduleResolution node --importHelpers --module es2015 --outDir ./dist/esm --sourceMap --declaration --declarationDir ./dist/types --target ES5 && del ts-files.txt",
    "postbuild": "rimraf dist/esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcGIS/rest-js.git"
  },
  "contributors": [
    {
      "name": "Patrick Arlt",
      "email": "parlt@esri.com",
      "url": "http://patrickarlt.com/"
    }
  ],
  "bugs": {
    "url": "https://github.com/ArcGIS/rest-js/issues"
  },
  "homepage": "https://github.com/ArcGIS/rest-js#readme"
}
