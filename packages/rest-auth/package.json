{
  "name": "@esri/rest-auth",
  "version": "0.0.0",
  "description": "Authentication helpers for @esri/rest-*.",
  "main": "dist/node/index.js",
  "browser": "dist/browser/rest-auth.umd.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "Apache-2.0",
  "dependencies": {
    "tslib": "^1.7.1"
  },
  "peerDependencies": {
    "@esri/rest-request": "0.0.0"
  },
  "devDependencies": {
    "@esri/rest-request": "0.0.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rimraf dist",
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc ./src/*.ts --moduleResolution node --importHelpers --module es2015 --outDir ./dist/esm --sourceMap --declaration --target ES5",
    "build:umd": "rollup -c ../../rollup.config.umd.js",
    "build:node": "tsc  ./src/*.ts --importHelpers --module commonjs --outDir ./dist/node --sourceMap --target ES5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcGIS/rest-js.git"
  },
  "contributors": [
    {
      "name": "Patrick Arlt",
      "email": "parlt@esri.com",
      "url": "http://patrickarlt.com/"
    }
  ],
  "bugs": {
    "url": "https://github.com/ArcGIS/rest-js/issues"
  },
  "homepage": "https://github.com/ArcGIS/rest-js#readme"
}
