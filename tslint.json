{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "strict-type-predicates": false,
    "ordered-imports": ["any"],
    "only-arrow-functions": [false],
    "object-literal-sort-keys": false,
    "interface-name": [true, "always-prefix"],
    "no-string-literal": false
  }
}
